<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RoundProgress">
        <attr name="lineHeight" format="dimension"></attr>
        <attr name="progress" format="float"></attr>
        <attr name="maxProgress" format="float"></attr>
        <attr name="roundColor" format="color"></attr>
        <attr name="arcColor" format="color"></attr>
        <attr name="textColor" format="color"></attr>
        <attr name="textSize" format="dimension"></attr>
    </declare-styleable>

    <!--导航标题属性-->
    <!--顶部导航栏 -->
    <declare-styleable name="commonTitleView">
        <!-- 是否显示左边的回退按钮-->
        <attr name="show_left" format="boolean"/>
        <!-- 左边按钮资源-->
        <attr name="left_drawable" format="reference"/>
        <!-- 标题内容 -->
        <attr name="title_context" format="string"/>
        <!-- 是否显示右侧 -->
        <attr name="show_right" format="boolean"/>
        <!-- 右边按钮资源-->
        <attr name="right_drawable" format="reference"/>
        <!--标题字体大小 -->
        <attr name="title_size" format="dimension"/>
        <!--标题颜色-->
        <attr name="title_color" format="color"/>
    </declare-styleable>
</resources>